{
  "name": "vite-ts-react-boilerplate",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,scss,md,json}'",
    "create-component": "node scripts/create-component.js",
    "delete-component": "node scripts/delete-component.js",
    "create-page": "node scripts/create-page.js",
    "delete-page": "node scripts/delete-page.js",
    "create-hook": "node scripts/create-hook.js",
    "delete-hook": "node scripts/delete-hook.js",
    "create-slice": "node scripts/create-slice.js",
    "delete-slice": "node scripts/delete-slice.js"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "@types/node": "^20.8.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
